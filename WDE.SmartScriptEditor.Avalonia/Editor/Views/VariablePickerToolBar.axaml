<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:WDE.SmartScriptEditor.Editor.ViewModels;assembly=WDE.SmartScriptEditor"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="viewModels:VariablePickerViewModel"
             x:Class="WDE.SmartScriptEditor.Avalonia.Editor.Views.VariablePickerToolBar">
    <ToolbarPanel>
        <ButtonImage Image="Icons/icon_add.png" Command="{CompiledBinding AddItemCommand}" />
        <ButtonImage Image="Icons/icon_delete.png" Command="{CompiledBinding RemoveItemCommand}" />
    </ToolbarPanel>
</UserControl>
